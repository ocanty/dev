{
  "name": "@tree/rpc",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup index.ts --format esm,cjs --dts --external react",
    "dev": "tsup index.ts --format esm,cjs --watch --dts --external react",
