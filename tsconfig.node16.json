{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 16",
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "lib": ["es2021"],
    "module": "Node16",
    "moduleResolution": "Node16",
    "target": "es2021",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}